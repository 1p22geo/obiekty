{
    "env": {
        "es6": true,
        "node": true,
        "browser":true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-unreachable": "error",
        "no-undef":"warn"
    }
